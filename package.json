    "color-hash": "^2.0.1",
    "flatstore": "^1.1.3",
    "framer-motion": "^7.9.1",
    "history": "^5.0.1",
    "http-proxy-middleware": "^1.0.6",
    "long": "^4.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-color": "^2.19.3",
    "react-custom-scrollbars-2": "^4.5.0",
