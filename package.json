{
  "name": "fsg-client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "flatstore": "^1.0.2",
    "fsg-shared": "file:../fsg-shared",
    "history": "^5.0.1",
    "http-proxy-middleware": "^1.0.6",
    "long": "^4.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "pako": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-images-uploading": "^3.1.2",
    "react-router-dom": "^5.2.0",
    "react-spring": "^9.2.4",
    "wait-on": "^5.3.0",
    "websocket": "^1.0.33"
  },
  "scripts": {
    "start": "./node_modules/.bin/concurrently npm:build:dev npm:sync",
    "build:dev": "./node_modules/.bin/webpack --config webpack/dev.config.js --watch ./src",
    "build:prod": "./node_modules/.bin/webpack --config webpack/prod.config.js",
    "sync": "./node_modules/.bin/wait-on http://localhost:8080/ && browser-sync start --no-ghost-mode --ws --port 8000 --proxy localhost:8080 --files=./src/* --files=../fsg-api/public/* --ignore=node_modules",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.14",
    "concurrently": "^6.1.0",
    "core-js": "^3.12.1",
    "css-loader": "^5.2.5",
    "sass": "^1.33.0",
    "sass-loader": "^11.1.1",
    "source-map-support": "^0.5.19",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  }
}
