    "sortablejs": "^1.15.0",
    "wait-on": "^5.3.0",
    "websocket": "^1.0.33"
  },
  "scripts": {
    "start": "npm run build:dev & npm run watch",
    "build:dev": "NODE_ENV=localhost npx webpack --config webpack/dev.config.js --watch ./src",
    "build:mobile": "NODE_ENV=mobile npx webpack --config webpack/dev.config.js --watch ./src",
    "build:prod": "NODE_ENV=production npx webpack --config webpack/prod.config.js",
    "watch": "npm run waiton && npm run sync",
    "waiton": "npx wait-on http://localhost:8080/",
    "sync": "npx browser-sync start --no-notify --no-ghost-mode --ws --port 8000 --proxy localhost:8080 --files=./src/* --files=../api/public/* --ignore=node_modules",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.11.2",
    "compression-webpack-plugin": "^9.2.0",
    "concurrently": "^6.1.0",
    "css-loader": "^6.5.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.33.0",
    "sass-loader": "^12.4.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^5.0.1"
  }
}
