{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "type":"module",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "npm run vite:dev",
     "vite:build": "vite build",
     "vite:dev": "vite serve",
     "build:dev": "NODE_ENV=localhost npx webpack --config webpack/dev.config.js --watch",
     "build:mobile": "NODE_ENV=mobile npx webpack --config webpack/dev.config.js --watch",
     "build:prod": "NODE_ENV=production npx webpack --config webpack/prod.config.js",
     "watch": "npm run waiton && npm run sync",
     "waiton": "npx wait-on http://localhost:8080/",
     "sync": "npx browser-sync start --no-ghost-mode --ws --port 8000 --proxy localhost:8080 --files=./src/* --files=../api/public/* --ignore=node_modules",
     "build": "react-scripts build",
     "test": "react-scripts test",
     "eject": "react-scripts eject"
   }, 
  "dependencies": {
    "@chakra-ui/icons": "^2.0.13",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^4.5.1",
    "@react-icons/all-files": "^4.1.0",
    "@uiw/react-md-editor": "^3.23.3",
    "@vitejs/plugin-react": "^4.2.1",
    "acos-json-delta": "^1.0.0",
    "acos-json-encoder": "^1.0.0",
    "axios": "^1.6.2",
    "cc-time-ago": "^2.0.3",
    "classnames": "^2.3.1",
    "color-hash": "^2.0.1",
    "flatstore": "^1.1.3",
    "framer-motion": "^10.16.16",
    "history": "^5.0.1",
    "http-proxy-middleware": "^1.0.6",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "long": "^4.0.0",
    "lottie-react": "^2.4.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-icons": "^5.1.0",
    "react-images-uploading": "^3.1.7",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.4.5",
    "react-select": "^5.8.0",
    "react-social-login-buttons": "^3.6.1",
    "rehype-sanitize": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "shared": "file:../shared",
    "simplebar-react": "^2.4.3",
    "vite": "^5.2.10",
    "wait-on": "^7.2.0",
    "websocket": "^1.0.33"
  },
  "devDependencies": {
    
    "concurrently": "^6.1.0",
    "react-refresh": "^0.14.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.69.5"
  }
}
